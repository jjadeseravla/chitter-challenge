<h1> Welcome to Chitter </h1>

<ul id='homepage'>
  <% @homepage.each do |homepage| %>
    <li>
      Message: <%= homepage.message %>
    </li>
  <% end %>
</ul>

<form action="/homepage/new_message" method="get">

  <input type="submit" value="Add new message">

</form>

